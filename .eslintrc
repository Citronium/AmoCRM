{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": ",@typescript-eslint/parser",
  "plugins": [
    "jest:recommended",
    "@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "jasmine": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": ["error", "always"]
  }
}
